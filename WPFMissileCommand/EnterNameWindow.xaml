<Window x:Name="enterNameWindow" x:Class="WPFMissileCommand.EnterNameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ENTER NAME" Height="129.93" Width="233.45" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="textBox" Margin="10,10,10,52" TextWrapping="Wrap" TextChanged="textBox_TextChanged"/>
        <Button x:Name="enterNameButton" Content="ENTER" Margin="115,55,10,10" Click="enterNameButton_Click" IsEnabled="False"/>
        <Button x:Name="cancelButton" Content="CANCEL" HorizontalAlignment="Left" Margin="10,55,0,10" Width="100" Click="cancelButton_Click"/>
    </Grid>
</Window>
